.i-res
    width 20px
    height 12px

.i-res-6
    height 16px

for r in (1..6)
    .i-res-{r}
        background-position center 20px * (r - 1)

res(version)
    .t{version} .i-res
        background-image url pathjoin(dpi_path(), "t" + version, "res.png")

res(2)
res(3)
res(4)
res(5)

.t5 .i-res
    width 16px
    height 16px